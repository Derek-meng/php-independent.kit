{
  "name": "derek-meng/php-independent.kit",
  "version": "2.0.0",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Derekmeng",
      "email": "a0985265734@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Independent\\Kit\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Independent\\Kit\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
